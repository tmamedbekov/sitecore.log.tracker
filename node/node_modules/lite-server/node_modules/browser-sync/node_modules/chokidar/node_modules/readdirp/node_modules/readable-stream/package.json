{
  "name": "readable-stream",
  "version": "2.2.0",
  "description": "Streams3, a user-land copy of the stream library from Node.js",
  "main": "readable.js",
  "dependencies": {
    "buffer-shims": "^1.0.0",
    "core-util-is": "~1.0.0",
    "isarray": "~1.0.0",
    "inherits": "~2.0.1",
